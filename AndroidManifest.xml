<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.morningreport"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.morningreport.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.morningreport.PCWActivity"
            android:label="@string/title_activity_pcw" >
        </activity>
        <activity
            android:name="com.example.morningreport.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.morningreport.ContactActivity"
            android:label="@string/title_activity_contact" >
        </activity>
        <activity
            android:name="com.example.morningreport.HelpActivity"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="com.example.morningreport.RedditActivity"
            android:label="@string/title_activity_reddit" >
        </activity>
        <activity
            android:name="com.example.morningreport.RSSActivity"
            android:label="@string/title_activity_rss" >
        </activity>
        <activity
            android:name="com.example.morningreport.RssAdderActivity"
            android:label="@string/title_activity_rss_adder" >
        </activity>
    </application>

</manifest>